{
  "name": "sbi-pdf",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "run-p build:*",
    "build:ts": "tsc",
    "build:gradle": "cd tabula && ./gradlew shadowJar && cd ..",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts src",
    "fix": "prettier --write src"
  },
  "dependencies": {
    "ajv": "7.0.2",
    "expand-tilde": "2.0.2",
    "p-limit": "3.1.0",
    "utility-types": "3.10.0",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@types/expand-tilde": "2.0.0",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
    "@types/yargs": "15.0.12",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "import-sort-style-module": "6.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-plugin-import-sort": "0.0.6",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
